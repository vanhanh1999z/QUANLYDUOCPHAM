			<Toolbar>
				<Title text="Thông tin chi tiết phần mềm" />
				<ToolbarSpacer />
				<OverflowToolbarButton icon="sap-icon://decline" press="onCloseHangDetail" />
			</Toolbar>
		</customHeader>
		<endButton>
