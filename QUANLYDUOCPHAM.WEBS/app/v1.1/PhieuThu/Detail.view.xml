    <core:FragmentDefinition controllerName="app.PhieuThu.Detail" xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:Form id="FormChange471" editable="true">
			<f:toolbar>
				<Toolbar id="TB1">
					<Title text="{mainModel>/id}" />
					<ToolbarSpacer />
					<Button icon="sap-icon://edit" press="onEditButtonPress" />
					<Button icon="sap-icon://delete" press="onDeleteButtonPress" />
				</Toolbar>
			</f:toolbar>

			<f:layout>
				<f:ResponsiveGridLayout labelSpanXL="4" labelSpanL="4" labelSpanM="4" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="1" emptySpanL="1" emptySpanM="1" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1" singleContainerFullSize="false" />
			</f:layout>
			<f:formContainers>
				<f:FormContainer>
					<f:formElements>
                        
    <f:FormElement label="ID">
        <f:fields>
            <Text text="{mainModel>/id}" id="maPhieu" />
        </f:fields>
    </f:FormElement>
        
    <f:FormElement label="Ngày thu">
        <f:fields>
            <Text text="{mainModel>/ngaythu}" id="ngayThu" />
        </f:fields>
    </f:FormElement>
        
    <f:FormElement label="Số tiền nộp">
        <f:fields>
            <Text text="{mainModel>/sotiennop}" id="soTienNop" />
        </f:fields>
    </f:FormElement>
        
    <f:FormElement label="Mã khách hàng">
        <f:fields>
            <Text text="{mainModel>/idkhach}" id="maKH" />
        </f:fields>
    </f:FormElement>
        
    <f:FormElement label="Tên quản lý">
        <f:fields>
            <Text text="{mainModel>/tenquanly}" id="tenQuanLy" />
        </f:fields>
    </f:FormElement>
        
    <f:FormElement label="Mã phiếu giao">
        <f:fields>
            <Text text="{mainModel>/idphieugiao}" id="idPhieuGiao" />
        </f:fields>
    </f:FormElement>
        
					</f:formElements>
				</f:FormContainer>
			</f:formContainers>
		</f:Form>
	</VBox>
</core:FragmentDefinition>
    